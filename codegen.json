{
  "overwrite": true,
  "schema": "./src/schema.ts",
  "generates": {
    "src/generated/schema.ts": {
      "plugins": ["typescript", "typescript-resolvers"],
      "config": {
        "contextType": "../resolvers/property.resolvers#GraphQLContext",
        "mappers": {
          "Property": "../db/types#PropertyEntity"
        },
        "skipTypename": true
      }
    }
  }
}
